<?xml version="1.0" encoding="utf-8" ?>
<sites>
	<reserved_header_section
		templateID="2001"
		templateFile="Sitemap.xml"
		templateDescription="Xml sitemap private configuration">
	</reserved_header_section>

	<!--
	Alberatura del sito
	- Nodo radice => <site id="<nome sito>" >
	- Nodo Home => <page id="home" url="...." text="Home Page">
	- Altri nodi => tutti figli di "home" secondo necessità


	Chiavi Dictionary:

	{{conf_edit_code}}   |   codice elemento lookup (generico)
	{{subject_id}}       |   codice soggetto riferimento
	{{subject_name}}     |   nome soggetto riferimento
	{{filter_month}}     |   mese riferimento (timesheet)
	{{filter_year}}      |   anno riferimento (timesheet)
	{{label_x1}}         |   etichetta X1 (timesheet)
	{{label_x2}}         |   etichetta X2 (timesheet)
	{{g1_text}}          |   etichetta gruppo1 (risorse umane)
	{{g2_text}}          |   etichetta gruppo2 (risorse umane)
	{{g3_text}}          |   etichetta gruppo3 (risorse umane)
	{{g4_text}}          |   etichetta gruppo4 (risorse umane)
	{{g5_text}}          |   etichetta gruppo5 (risorse umane)
	{{g6_text}}          |   etichetta gruppo6 (risorse umane)
	{{g7_text}}          |   etichetta gruppo7 (risorse umane)
	{{g8_text}}          |   etichetta gruppo8 (risorse umane)
	{{g9_text}}          |   etichetta gruppo9 (risorse umane)
	{{g10_text}}         |   etichetta gruppo10 (risorse umane)

	-->


	<site id="pmcloud">
		<page id="home" url="/home" text="Home Page">

			<!-- ===== AZIENDE =========================================== -->
			<page id="ru_conf_aziende" url="/configuration/aziende" text="Configurazione - Aziende">
				<page id="ru_conf_aziende_edit" url="/configuration/azienda/{{conf_edit_code}}" text="Modifica"></page>
			</page>

			<!-- ===== HUMAN RESOURCES =================================== -->
			<page id="hr_elenco_soggetti" url="/hr/elenco-soggetti" text="Elenco soggetti in Azienda (Attivi)">
			</page>

			<page id="elenco_soggetti" url="/ElencoSoggetti/ElencoSoggetti.aspx" text="Elenco Soggetti in Azienda (Attivi)">
				<page id ="dettaglio_soggetto" url="/anagrafica/soggetto.aspx?ID={{subject_id}}" text="{{subject_name}}"></page>
			</page>
			<page id="elenco_soggetti_passati" url="/ElencoSoggetti/ElencoSoggettiPassati.aspx" text="Elenco Soggetti NON in Azienda (Cessati)">
				<page id ="dettaglio_soggetto" url="/anagrafica/soggetto.aspx?ID={{subject_id}}" text="{{subject_name}}"></page>
			</page>
			<page id="elenco_soggetti_futuri" url="/ElencoSoggetti/ElencoSoggettiFuturi.aspx" text="Elenco Soggetti NON in Azienda (Futuri)">
				<page id ="dettaglio_soggetto" url="/anagrafica/soggetto.aspx?ID={{subject_id}}" text="{{subject_name}}"></page>
			</page>
				<page id="elenco_soggetti_tutti" url="/ElencoSoggetti/ElencoSoggettiTutti.aspx" text="Elenco Soggetti Completo (Attivi, Cessati, Futuri)">
			<page id ="dettaglio_soggetto" url="/anagrafica/soggetto.aspx?ID={{subject_id}}" text="{{subject_name}}"></page>
			</page>
			<page id="elenco_soggetti_archivio" url="/ElencoSoggetti/Archivio.aspx" text="Archivio">
				<page id ="dettaglio_soggetto" url="/anagrafica/soggetto.aspx?ID={{subject_id}}" text="{{subject_name}}"></page>
			</page>

			<!-- ===== SCADENZIARIO ====================================== -->
			<page id="sc_elenco_scadenze" url="/scadenziario/gestione-scadenze" text="Gestione scadenze">
				<page id="sc_scadenza_cre" url="/scadenziario/scadenza" text="Crea nuova scadenza"></page>
				<page id="sc_scadenza_upd" url="/scadenziario/scadenza/{{conf_edit_code}}" text="Modifica scadenza"></page>
				<page id="sc_scadenza_exe" url="/scadenziario/esegui/{{conf_edit_code}}" text="Esegui scadenza"></page>
			</page>
			<page id="elencoscadenze" url="/Scadenziario/ElencoScadenze_Manutenzione.aspx" text="Scadenziario - Elenco Scadenze Manutenzione">
				<page id="scadenza_gestione" url="/Scadenziario/GestioneScadenza.aspx?COD={{deadline_code}}" text="Gestione Scadenza"></page>
				<page id="scadenza_esecuzione" url="/Scadenziario/EsecuzioneScadenza.aspx?COD={{deadline_code}}&amp;F={{f_code}}" text="Esecuzione Scadenza"></page>
			</page>

			<!-- ====== TIMESHEET ======================================== -->
			<page id="ts_elenco_soggetti" url="/timesheet/view" text="Elenco Timesheet">
